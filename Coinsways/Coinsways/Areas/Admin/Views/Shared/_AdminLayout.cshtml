<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title - Coinways</title>
    <link rel="shortcut icon" href="~/Images/favicon.png" type="image/x-icon">
    <meta charset="utf-8" />
    <link rel="apple-touch-icon" sizes="76x76" href="~/Images/apple_icon.jpg" />
    <link rel="icon" type="image/png" href="~/Images/favicon.jpg" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0" name="viewport" />
    <meta name="viewport" content="width=device-width" />

    <!-- Bootstrap core CSS     -->
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/Dashboard/material-dashboard.css" rel="stylesheet" />
    <link href="~/Content/Dashboard/demo.css" rel="stylesheet" />
    <link href="~/Content/DataTables/css/dataTables.bootstrap.css" rel="stylesheet" />
    <link href="~/Content/bootstrapchanges.less" rel="stylesheet" />
    <link href="~/Content/DataTables/css/responsive.dataTables.css" rel="stylesheet" />
    <link href="http://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css" rel="stylesheet" />
    <link href="http://fonts.googleapis.com/css?family=Roboto:400,700,300|Material+Icons" rel="stylesheet" type="text/css" />
    <script src="~/Scripts/jquery-3.1.0.min.js" type="text/javascript"></script>
    <script src="~/Scripts/bootstrap.min.js" type="text/javascript"></script>
    <script src="~/Scripts/Dashboard/material.min.js" type="text/javascript"></script>
    <script src="~/Scripts/Dashboard/chartist.min.js"></script>
    <script src="~/Scripts/DataTables/jquery.dataTables.js"></script>
    <script src="~/Scripts/DataTables/dataTables.responsive.js"></script>
    <script src="~/Scripts/Dashboard/bootstrap-notify.js"></script>
    @*<script type="text/javascript" src="https://maps.googleapis.com/maps/api/js"></script>*@
    <script src="~/Scripts/Dashboard/material-dashboard.js"></script>
    <script src="~/Scripts/Dashboard/demo.js"></script>
</head>
<body>
    <div class="wrapper">

        <div class="sidebar" data-color="purple" data-image="img/sidebar-1.jpg">
            <!--
                Tip 1: You can change the color of the sidebar using: data-color="purple | blue | green | orange | red"

                Tip 2: you can also add an image using data-image tag
            -->

            <div class="logo">
                <a href="#" class="simple-text">
                    <img src="~/Images/logo_bg.png" class="img-responsive" />
                </a>
            </div>

            <div class="sidebar-wrapper">
                <ul class="nav">
                    <li class="active">
                        <a href="@Url.Action("Index", "Main", new { @Area = ""})">
                            <i>GoBack</i>
                            <p>&nbsp;</p>
                        </a>
                    </li>

                    <li class="dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown" aria-expanded="true">Masters <span class="caret"></span></a>
                        <ul class="dropdown-menu drop-menu">
                            <li><a href="@Url.Action("Index", "AspNetRole", new { @Area = "Admin"})">User Type</a></li>
                            <li><a href="@Url.Action("Index", "AspNetUser", new { @Area = "Admin"})">Users</a></li>
                            <li><a href="@Url.Action("Index", "ComissionType", new { @Area = "Admin"})">Comission Type</a></li>
                            <li><a href="@Url.Action("Index", "CurrencyCode", new { @Area = "Admin"})">Currency Master</a></li>
                            <li><a href="@Url.Action("Index", "DailyCommission", new { @Area = "Admin"})">Daily Commission</a></li>
                            <li><a href="@Url.Action("Index", "LevelCommission", new { @Area = "Admin"})">Level Commission</a></li>
                            <li><a href="@Url.Action("Index", "PaymentMode", new { @Area = "Admin"})">Payment Mode</a></li>
                            <li><a href="@Url.Action("Index", "PlanDetail", new { @Area = "Admin"})">Plan Master</a></li>
                            <li><a href="@Url.Action("Index", "HelpType", new { @Area = "Admin"})">Help Type</a></li>
                            <li><a href="@Url.Action("Index", "HelpDesk", new { @Area = "Admin"})">Help Desk</a></li>
                        </ul>
                    </li>

                    <li class="dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown" aria-expanded="true">Transaction Master<span class="caret"></span></a>
                        <ul class="dropdown-menu drop-menu">
                            <li><a href="@Url.Action("Index", "TypeOfTransaction", new { @Area = "Admin"})">Transaction Type</a></li>
                            <li><a href="@Url.Action("Index", "TransactionStatu", new { @Area = "Admin"})">Transaction Status</a></li>
                        </ul>
                    </li>
                </ul>
            </div>
        </div>
        <div class="main-panel">
            @using (Html.BeginForm("LogOff", "Account", new { Area = "" }, FormMethod.Post, new { id = "logoutForm" }))
            {
                @Html.AntiForgeryToken()
                <nav class="navbar navbar-transparent navbar-absolute">
                    <div class="container-fluid">
                        <div class="navbar-header">
                            <button type="button" class="navbar-toggle" data-toggle="collapse">
                                <span class="sr-only">Toggle navigation</span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                            </button>

                        </div>
                        <div class="collapse navbar-collapse">
                            <ul class="nav navbar-nav navbar-right">
                                <li class="dropdown">
                                    <a href="#" class="dropdown-toggle fhgvswe" data-toggle="dropdown">
                                        <img src="img/mini-profile.png" class="mini-profi" />
                                        <span id="ctl00_lblmail">@User.Identity.Name</span>
                                    </a>
                                </li>
                                <li>
                                    <a href="javascript:document.getElementById('logoutForm').submit()" class="ftuif">Logout</a>
                                </li>
                            </ul>
                        </div>
                    </div>
                </nav>
            }

            @RenderBody()
        </div>
    </div>
</body>
</html>
@RenderSection("scripts", required: false)
<script type="text/javascript">
    $(document).ready(function () {

        // Javascript method's body can be found in assets/js/demos.js
        demo.initDashboardPageCharts();

    });
</script>